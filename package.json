{
  "name": "ma-front",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run webpack",
    "build-dev": "npm run clean-dist && npm run webpack && npm run copy",
    "copy": "cp src/utils/mockListBookmarks.json public/mockListBookmarks.json",
    "webpack": "webpack --config ./webpack.config.js --mode production --devtool 'source-map'",
    "clean-dist": "rm -f -r -d public",
    "lint": "node_modules/.bin/eslint --config=.eslintrc",
    "lint-all": "node_modules/.bin/eslint --config=.eslintrc src/**/*.js src/**/*.jsx",
    "pretty:all": "prettier-eslint --write \"./src/**/*.{js,jsx}\"",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --devtool 'eval-source-map'",
    "test": "jest",
    "watch": "jest --watch",
    "coverage:all": "jest --coverage",
    "coverage": "echo -n \"-> Coverage for $FILE: \" && jest --coverage 2>/dev/null | grep $FILE | cut -d '|' -f2 | tr -d ' ' && echo ''"
  },
  "author": "",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.4.2",
    "msw": "^0.32.0",
    "node-sass": "^4.12.0",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.14.0",
    "loglevel": "^1.6.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.32.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "use-http": "^1.0.21"
  }
}
